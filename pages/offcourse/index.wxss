page {
  padding-bottom: calc(50px + env(safe-area-inset-bottom));
}

.no-choose-wrap {
  padding: 20rpx;
}

.info {
  position: relative;
  font-weight: bold;
  display: inline-block;
  padding: 12rpx 20rpx 0;
}
.info .banner {
  width: 710rpx;
  height: 360rpx;
  margin-bottom: 17rpx;
  border-radius: 20rpx;
}
.info .ctn {
  background-color: #ffffff;
  padding: 32rpx 28rpx;
  border-radius: 20rpx;
}
.link {
  color: #1A1A1A;
}
.info .title {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.info .title .text {
  font-size: 36rpx;
  font-weight: 800;
  color: #1A1A1A;
  flex: 1;
}
.info .title .tag {
  font-size: 24rpx;
  color: #1A1A1A;
  margin-top: 10rpx;
  width: 133rpx;
  text-align: center;
  height: 54rpx;
  line-height: 54rpx;
  background: #F3F3F3;
  border-radius: 27rpx;
  z-index: 1;
  position: relative;
}
.info .desc {
  font-size: 26rpx;
  color: #1A1A1A;
  margin: 24rpx 0 20rpx;
}
.info .bottom {
  display: flex;
  justify-content: space-between;
}
.info .bottom .tag {
  height: 40rpx;
  line-height: 40rpx;
  background: #EFF8FF;
  border-radius: 8rpx;
  padding: 0 17rpx;
  font-size: 22rpx;
  color: #1A94FB;
 }
.btn-wrap {
  display: flex;
  font-size: 24rpx;
  background-color: #ffffff;
}
.btn-wrap .btn {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}
.btn-wrap image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 10rpx;
}


.card {
  width: 710rpx;
  background: #FFFFFF;
  border-radius: 20rpx;
  margin: 20rpx auto;
  padding: 30rpx 25rpx;
}
.card.type1 {
  margin-top: 0;
  border-radius: 0 0 20rpx 20rpx;
}
.card2 {
  padding: 30rpx 0;
}
.card2 .page-title {
  padding: 0 25rpx;
  margin-bottom: 0;
}
.card2 .page-title::before {
  left: 25rpx;
}
.page-title {
  font-size: 36rpx;
  font-weight: 800;
  color: #1A1A1A;
  margin-bottom: 30rpx;
  position: relative;
}
.page-title::before {
  width: 140rpx;
  height: 9rpx;
  content: '';
  display: block;
  background: #FFD925;
  position: absolute;
  left: 0;
  bottom: 1rpx;
}

.total-wrap {
  text-align: center;
  font-size: 24rpx;
  color: #333;
  display: flex;
  justify-content: space-between;
  color: #fff;
}
.total-wrap .bage-tag {
  border-radius: 16rpx;
  background: #FFD925;
  border-radius: 0 4rpx 4rpx 0;
  height: 33rpx;
  line-height: 33rpx;
  font-size: 20rpx;
  color: #644931;
  padding: 0 11rpx;
  position: absolute;
  top: 20rpx;
  left: 20rpx;
}
.total-wrap .item {
  position: relative;
  width: 318rpx;
  height: 261rpx;
  padding-top: 68rpx;
  margin-right: 18rpx;
  position: relative;
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100%;
  background-image: url(https://image.jkcspace.com/wxmini_static/images/offcourse-member-bg1.png);
}
.total-wrap .item:last-child {
  margin-right: 0;
}
.total-wrap .item .title {
  font-size: 28rpx;
  line-height: 1;
  margin-bottom: 16rpx;
  font-weight: bold;
  color: #644931;
}
.total-wrap .bottom {
  min-height: 53rpx;
  display: flex;
  justify-content: center;
  font-size: 30rpx;
  color: #907541;
}
.total-wrap .text {
  font-size: 22rpx;
}
.total-wrap .text-wrap {
  display: flex;
  justify-content: center;
  align-items: flex-end;
  margin-right: 20rpx;
  font-size: 26rpx;
}
.total-wrap .strong {
  font-weight: bold;
  font-size: 48rpx;
  line-height: 48rpx;
  color: #FF502F;
  margin-right: 10rpx;
}
.total-wrap .item .money {
  display: flex;
  justify-content: center;
  align-items: flex-end;
  text-decoration: line-through;
  font-size: 30rpx;
}
.total-wrap .item .num {
  font-size: 24rpx;
  display: inline-block;
}
.total-wrap .item .btn {
  width: 220rpx;
  height: 52rpx;
  margin-top: 26rpx;
}
.total-wrap .item2 {
  position: relative;
  width: 320rpx;
  height: 150rpx;
  padding-top: 42rpx;
  padding-left: 27rpx;
  margin-right: 17rpx;
  margin-top: 30rpx;
  position: relative;
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100%;
  background-image: url(https://image.jkcspace.com/wxmini_static/images/offcourse-type-bg1.png);
}
.total-wrap navigator:last-child .item2 {
  background-image: url(https://image.jkcspace.com/wxmini_static/images/offcourse-type-bg2.png);
}
.total-wrap .item2 .title {
  font-size: 28rpx;
  color: #1A1A1A;
  font-weight: bold;
  margin-bottom: 14rpx;
  text-align: left;
}
.total-wrap .item2 .desc {
  font-size: 20rpx;
  font-weight: 400;
  color: #999999;
  text-align: left;
}



/* 未开课 预约 */
.yuyue {
  padding: 90rpx 0 60rpx;
  text-align: center;
  font-size: 30rpx;
  margin-top: 0;
  border-radius: 0 0 20rpx 20rpx;
}
.yuyue .image {
  width: 246rpx;
  height: 166rpx;
}
.yuyue .title {
  font-weight: 400;
  color: #999999;
  margin: 30rpx auto 40rpx;
}
.yuyue .btn {
  width: 350rpx;
  height: 86rpx;
  line-height: 86rpx;
  border-radius: 43rpx;
  margin: 0 auto;
  background: #FFD925;
  font-size: 30rpx;
  font-weight: 800;
  color: #1A1A1A;
}

.date-wrap {
  margin-top: 40rpx;
  padding: 0 20rpx;
}
/* 年纪tag */
.tag-wrap {
  display: flex;
  font-size: 24rpx;
  background-color: #ffffff;
}
.tag-wrap .tag {
  padding: 0 30rpx;
  margin-right: 10rpx;
  height: 48rpx;
  line-height: 48rpx;
  background: #F7F7F7;
  border-radius: 24rpx;
  font-size: 24rpx;
  color: #2A2B2D;
}
.tag-wrap .tag.select {
  background: #FFD925;
  font-weight: bold;
}

.list-wrap {
  font-size: 24rpx;
  padding: 24rpx 0;
  margin: 0 20rpx 20px;
  background: #fff;
  border-radius: 20rpx;
}
.list-wrap .item {
  width: 100%;
  margin-bottom: 25rpx;
  border-bottom: 1rpx solid #F3F3F3;
  padding: 24rpx 0;
  display: flex;
  align-items: flex-start;
  position: relative;
}
.list-wrap .item:last-child {
  border-bottom: 0;
  margin-bottom: 0;
}
.list-wrap .item .cover {
  width: 244rpx;
  height: 175rpx;
  border-radius: 20rpx;
  margin-right: 24rpx;
}
.list-wrap .item .bage-tag {
  background: rgba(0, 0, 0, 0.45);
  border-radius: 16rpx;
  height: 33rpx;
  line-height: 33rpx;
  font-size: 20rpx;
  color: #FFFFFF;
  padding: 0 11rpx;
  position: absolute;
  top: 44rpx;
  left: 20rpx;
}
.list-wrap .item .bage-tag2 {
  height: 37rpx;
  line-height: 37rpx;
  border-radius: 20rpx 0px 16rpx 0px;
  font-size: 22rpx;
  color: #FFFFFF;
  padding: 0 11rpx;
  position: absolute;
  left: 158rpx;
  top: 163rpx;
}
.list-wrap .item .bage-tag2.bg1 {
  background: linear-gradient(-50deg, #FF742F, #FBBE2A);
}
.list-wrap .item .bage-tag2.bg2 {
  background: linear-gradient(-50deg, #0DB9A7, #10C889);
}
.list-wrap .item .bage-tag2.bg3 {
  background: linear-gradient(-50deg, #2F87FF, #00C0FF);
}
.list-wrap .item .ctn {
  flex: 1;
  font-size: 26rpx;
  color: #1A1A1A;
}
.list-wrap .item view {
  line-height: 26rpx;
}
.list-wrap .title {
  font-size: 30rpx;
  line-height: 30rpx;
  margin-bottom: 16rpx;
}
.list-wrap .names {
  display: flex;
  align-items: center;
  color: #666;
}
.list-wrap .names .line:first-child {
  margin-right: 12rpx;
  margin-left: 0;
}
.list-wrap .line {
  color: #999999;
  margin-top: 12rpx;
}
.list-wrap .bottom {
  margin-top: 20rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.list-wrap .btn {
  background: #FFD925;
  box-shadow: 5rpx 0px 21rpx 0px rgba(0,0,0,0.07);
  border-radius: 40rpx;
  color: #1A1A1A;
  margin-left: 14rpx;
  padding: 18rpx 24rpx;
}
.list-wrap .btn.ghost {
  background: rgba(255,217,37,0);
  border: 1px solid #242424;
}

.dialpg-age {
  z-index: 999;
}

/* 班级tab */
.tabs {
  background-color: #fff;
  margin: 20rpx auto 0;
  width: 710rpx;
  border-radius: 20rpx;
}