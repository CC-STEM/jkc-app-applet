.out-tabs-wrap {
  width: 750rpx;
  background-color: #111323;
  box-shadow: 2rpx -21rpx 9rpx 0px rgba(0,0,0,0.05);
  display: flex;
  align-items: center;
  padding-top: 20rpx;
  font-weight: 400;
}
.out-tabs-wrap .out-tab {
  height: 77rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  width: 33.3%;
} 
.out-tabs-wrap .out-tab .image {
  width: 100rpx;
  height: 27rpx;
  margin: auto;
  display: block;
}
.out-tabs-wrap .out-tab:nth-child(2) .image {
  width: 166rpx;
  height: 27rpx;
}
.out-tabs-wrap .out-tab.active {
  height: 98rpx;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  background: linear-gradient(180deg, #FFFFFF, #F2F2F2);
  color: #1a1818;
  margin-top: 0rpx;
  padding-top: 16rpx;
}
.out-tabs-wrap .out-tab.active .image {
  position: relative;
  top: -6rpx;
}
.out-tabs-wrap .order-line {
  width: 42rpx;
  height: 15rpx;
  position: absolute;
  bottom: 6rpx;
  left: 0;
  right: 0;
  margin: auto;
}

.tabs-wrap {
  display: flex;
  align-items: center;
  padding: 0 23rpx;
}
.tab {
  position: relative;
  font-size: 30rpx;
  font-weight: 800;
  color: #0B0B0B;
  width: 354rpx;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-repeat: no-repeat;
  background-position: center;
  background-size: 100%;
}
.tab:nth-child(1) {
  background-image: url('https://image.jkcspace.com/wxmini_static/images/buy-bg-left.png');
}
.tab:nth-child(2) {
  background-image: url('https://image.jkcspace.com/wxmini_static/images/buy-bg-right.png');
}
.tab:nth-child(1).active {
  background-image: url('https://image.jkcspace.com/wxmini_static/images/buy-bg-left-active.png');
}
.tab:nth-child(2).active {
  background-image: url('https://image.jkcspace.com/wxmini_static/images/buy-bg-right-active.png');
}
text {
