.list-wrap {
  padding: 28rpx 24rpx 0;
  margin-bottom: 24rpx;
  font-size: 28rpx;
  color: #4F433B;
  border-radius: 20rpx 20rpx 0 0;
  background: linear-gradient(180deg, #FFFFFF, #F5F5F5);
  /* display: flex;
  align-items: center;
  flex-wrap: nowrap; */
}
.list-wrap .item {
  width: 206rpx;
  height: 369rpx;
  border-radius: 22rpx;
  padding: 36rpx 24rpx 0;
  margin-right: 12rpx;
  margin-bottom: 28rpx;
  position: relative;
  display: inline-block;
  border: 2rpx solid #252525;
  background: #fff;
  text-align: center;
  vertical-align: middle;
}
.list-wrap .item .title {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
 }
.list-wrap .item.active {
  background: linear-gradient(79deg, #F9EFEA, #FFE4DA);
  border: 2rpx solid #FF7505;
}
.list-wrap .item.active .bottom {
  color: #6F6051;
  background: #F6D2AB;
}
.list-wrap .item .bage-tag {
  background: linear-gradient(91deg, #FF5534, #FDAF8B);
  border-radius: 10rpx 0px 10rpx 0px;
  position: absolute;
  top: -11rpx;
  left: -1rpx;
  font-size: 22rpx;
  font-weight: 500;
  color: #FFFFFF;
  text-align: center;
  width: 77rpx;
  height: 32rpx;
  line-height: 32rpx;
}
.list-wrap .item .money-wrap {
  margin: 12rpx 0;
  font-size: 28px;
  color: #4F433B;
}
.list-wrap .item .text {
  font-size: 24rpx;
}
.list-wrap .item .text-wrap {
  display: flex;
  justify-content: center;
  align-items: flex-end;
  margin-right: 16rpx;
  color: #333333;
}
.list-wrap .item .strong {
  font-weight: bold;
  font-size: 48rpx;
  line-height: 48rpx;
  color: #FF502F;
}
.list-wrap .item .money {
  display: flex;
  justify-content: center;
  align-items: flex-end;
  text-decoration: line-through;
  font-size: 28rpx;
  color: #FFB396;
}
.list-wrap .item .num {
  font-size: 24rpx;
  display: inline-block;
}
.list-wrap .line {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  color: #333333;
}
.list-wrap .line .right {
  display: flex;
  align-items: center;
}
.list-wrap .line .strong {
  font-weight: bold;
  font-size: 30rpx;
  color: #1A1A1A;
  line-height: 1;
  margin-right: 2rpx;
  position: relative;
  top: -2rpx;
}
.list-wrap .item .bottom {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  bottom: 0;
  left: -0;
  font-size: 24rpx;
  font-weight: 500;
  color: #3D3D3D;
  background: #E3E3E3;
  border-radius: 0 0 20rpx 20rpx;
}
